{
  "name": "glyph-drawing-club-blog",
  "version": "1.0.0",
  "description": "A blog for Glyph Drawing Club",
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "run-s clean eleventy:dev --print-label",
    "build": "run-s clean eleventy:prod --print-label",
    "eleventy:dev": "cross-env ELEVENTY_ENV=development eleventy --serve",
    "eleventy:prod": "cross-env ELEVENTY_ENV=production eleventy",
    "clean": "del-cli dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hlotvonen/glyph-drawing-club-blog"
  },
  "author": "Heikki Lotvonen",
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^1.0.0-beta.8",
    "@11ty/eleventy-img": "^1.0.0",
    "@11ty/eleventy-navigation": "^0.3.2",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@orchidjs/eleventy-plugin-ids": "^0.1.1",
    "babel-loader": "^8.2.3",
    "clean-css": "^5.2.2",
    "critical": "^4.0.1",
    "cssesc": "^3.0.0",
    "del-cli": "^4.0.1",
    "focus-trap": "^6.7.1",
    "focus-visible": "^5.2.0",
    "html-minifier": "^4.0.0",
    "luxon": "^2.1.1",
    "markdown-it": "^12.2.0",
    "memfs": "^3.4.0",
    "node-sass": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "webpack": "^5.65.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3"
  }
}
